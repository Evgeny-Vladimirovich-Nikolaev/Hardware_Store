<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="Create-table-computer_accessories" author="Evgeny Nikolaev">
        <preConditions>
            <not>
                <tableExists tableName="computer_accessories"/>
            </not>
        </preConditions>
        <createTable tableName="computer_accessories">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="modelId" type="VARCHAR(60)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="code" type="VARCHAR(20)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="type" type="VARCHAR(60)">
                <constraints nullable="false"/>
            </column>
            <column name="price" type="DECIMAL">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="init-1" author="Evgeny Nikolaev">
        <insert tableName="computer_accessories">
            <column name="id" value="1">
            </column>
            <column name="modelId" value="C">
            </column>
            <column name="code" value="CD-ROM">
            </column>
            <column name="type" value="Compact Disc Read-Only Memory">
            </column>
            <column name="price" value="123">
            </column>
        </insert>
        <insert tableName="computer_accessories">
            <column name="id" value="2">
            </column>
            <column name="modelId" value="d">
            </column>
            <column name="code" value="CD-ROM">
            </column>
            <column name="type" value="Compact Disc Read-Only Memory">
            </column>
            <column name="price" value="123">
            </column>
        </insert>
        <insert tableName="computer_accessories">
            <column name="id" value="3">
            </column>
            <column name="modelId" value="e">
            </column>
            <column name="code" value="CD-ROM">
            </column>
            <column name="type" value="Compact Disc Read-Only Memory">
            </column>
            <column name="price" value="123">
            </column>
        </insert>

    </changeSet>


</databaseChangeLog>